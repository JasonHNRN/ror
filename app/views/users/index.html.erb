<%= render partial: "shared/menu", locals: {val_1: 'value_1'} %>

<h1>用户列表</h1>
<div class="row">
    <div class="col-sm-12">
        <table class="table">
            <tr>
                <th>ID</th>
                <th>用户名</th>
                <th></th>
            </tr>
            <% @users.each do |user| %>
              <tr>
                  <td><%= user.id %></td>
                  <td><%= user.username %></td>
              </tr>
            <% end -%>
        </table>

        <%= will_paginate @users %>
    </div>
</div>